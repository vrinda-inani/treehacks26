# Agent identity (use fixed seed for stable address in README)
AGENT_ORCHESTRATOR_SEED=hackoverflow-orchestrator-agent-seed-phrase
AGENT_COORDINATOR_SEED=hackoverflow-coordinator-agent-seed-phrase
AGENT_SPECIALIST_SEED=hackoverflow-specialist-agent-seed-phrase
AGENT_PORT_ORCHESTRATOR=8100
AGENT_PORT_COORDINATOR=8101
AGENT_PORT_SPECIALIST=8102
AGENT_PORT_ROUTER=8103
AGENT_PORT_EXPERT=8104
AGENT_PORT_STUCK_EXAMPLE=8105
AGENT_PORT_CURATOR=8106
AGENT_ROUTER_SEED=hackoverflow-router-agent-seed-phrase
AGENT_EXPERT_SEED=hackoverflow-expert-agent-seed-phrase
AGENT_STUCK_EXAMPLE_SEED=hackoverflow-stuck-example-seed-phrase
AGENT_CURATOR_SEED=hackoverflow-curator-agent-seed-phrase
# Set after first run (copy addresses from logs)
ORCHESTRATOR_AGENT_ADDRESS=
SPECIALIST_AGENT_ADDRESS=
COORDINATOR_AGENT_ADDRESS=
EXPERT_AGENT_ADDRESS=
ROUTER_AGENT_ADDRESS=
CURATOR_AGENT_ADDRESS=
# User-connected agents: handle:address, e.g. mybot:agent1q...
CONNECTED_AGENTS=

# Agentverse (required for mailbox and registration)
AGENTVERSE_API_KEY=your_agentverse_api_key
# Use mailbox transport (Agentverse/ASI:One). Set false for local direct-to-direct agent testing.
AGENT_MAILBOX_ENABLED=true
# Local endpoint host used when AGENT_MAILBOX_ENABLED=false.
AGENT_ENDPOINT_HOST=127.0.0.1

# ASI:One API (optional; for coordinator to use ASI:One model in responses)
# Get key at https://asi1.ai/dashboard/api-keys
ASI_ONE_API_KEY=

# OpenAI (optional fallback if not using ASI:One API)
OPENAI_API_KEY=

# Vercel AI Gateway (optional coordinator digest/status path; single key + fallback models)
# Docs: https://vercel.com/docs/ai-gateway
AI_GATEWAY_ENABLED=true
AI_GATEWAY_API_KEY=
AI_GATEWAY_BASE_URL=https://ai-gateway.vercel.sh/v1
AI_GATEWAY_PRIMARY_MODEL=openai/gpt-4o-mini
AI_GATEWAY_FALLBACK_MODELS=anthropic/claude-3.5-haiku,google/gemini-2.0-flash-001

# Optional grounding: pull relevant HackOverflow Q&A before model response
# For local backend in this repo, keep this as http://127.0.0.1:8000
HACKOVERFLOW_API_URL=http://127.0.0.1:8000
AI_GATEWAY_PULL_CONTEXT=true
AI_GATEWAY_CONTEXT_QUESTIONS=2
AI_GATEWAY_CONTEXT_QUESTION_CHARS=220
AI_GATEWAY_CONTEXT_ANSWER_CHARS=260

# Claude Agent SDK triage (optional; used by curator role only)
CLAUDE_TRIAGE_ENABLED=false
CLAUDE_TRIAGE_ALLOWED_TOOLS=
CLAUDE_TRIAGE_MAX_TOKENS=400
ANTHROPIC_API_KEY=

# RunPod Flash SDK sidecar (optional; does NOT replace Fetch.ai routing)
# Used only by Expert agent for triage hints in Router -> Expert flow.
RUNPOD_EXPERT_ENABLED=false
RUNPOD_API_KEY=
RUNPOD_FLASH_ENDPOINT_NAME=hackoverflow-expert-flash
RUNPOD_FLASH_GPU_GROUP=ANY
RUNPOD_FLASH_WORKERS_MIN=0
RUNPOD_FLASH_WORKERS_MAX=1
RUNPOD_FLASH_IDLE_TIMEOUT_MIN=5
RUNPOD_FLASH_HF_MODEL=Qwen/Qwen2.5-0.5B-Instruct
RUNPOD_FLASH_MAX_NEW_TOKENS=180

# Fetch.ai network for payment (true = testnet, false = mainnet)
FET_USE_TESTNET=true

# Agent-to-agent diagnostics traffic (visible in Agentverse Inspector)
AGENT_STARTUP_SIGNAL_ENABLED=true
AGENT_HEARTBEAT_ENABLED=true
AGENT_HEARTBEAT_SECONDS=45
